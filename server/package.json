{
  "name": "typescript-node-project",
  "version": "1.0.0",
  "description": "A modern Node.js TypeScript project with ES modules",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/trcp/index.ts | pino-pretty",
    "build": "tsc",
    "format": "biome format --write ./src/",
    "lint": "biome lint --write ./src/",
    "type-check": "tsc -b",
    "biome": "biome check --write ./src/",
    "start": "node dist/trcp/index.js",
    "start-tasks": "node dist/tasks/index.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.0",
    "@types/node": "22.13.5",
    "nodemon": "3.1.9",
    "pino-pretty": "^13.0.0",
    "tsx": "4.19.3",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@trpc/server": "11.0.0-rc.808",
    "cors": "^2.8.5",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "limiter": "3.0.0",
    "mongodb": "6.13.1",
    "pino": "^9.6.0",
    "zod": "3.24.2"
  }
}
